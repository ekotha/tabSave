{
    "manifest_version": 3,
    "name": "TabSave",
    "version": "1.0",
    "description": "Copy the URLs of all open tabs",
    "permissions": ["clipboardWrite", "activeTab", "tabs", "downloads"],
    "action": {
      "default_popup": "popup.html",
      "default_title": "TabSave"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  